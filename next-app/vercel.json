{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/execute/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/gemini/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/llama/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/swarm/**/*.ts": {
      "maxDuration": 300
    },
    "app/api/system/**/*.ts": {
      "maxDuration": 30
    }
  }
}
