{
  "name": "gemini-hydra-core",
  "version": "14.0.0",
  "description": "GeminiHydra Agent Swarm - School of the Wolf Edition",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "gemini": "./bin/gemini.ts"
  },
  "scripts": {
    "start": "node dist/bin/gemini.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:gui": "cd GeminiGUI && npm test",
    "test:all": "vitest run && cd GeminiGUI && npm test",
    "prestart": "npm run build",
    "prepare": "husky",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "agent-swarm",
    "ai",
    "automation",
    "gemini",
    "llama-cpp"
  ],
  "author": "Regis",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@google/gemini-cli": "^0.1.1",
    "@google/gemini-cli-core": "^0.25.2",
    "@google/generative-ai": "^0.24.1",
    "@inquirer/prompts": "^7.0.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "chalk": "^5.6.2",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.2",
    "glob": "^10.3.10",
    "micromatch": "^4.0.8",
    "ollama": "^0.5.18",
    "ora": "^9.3.0",
    "p-limit": "^5.0.0",
    "p-queue": "^8.0.1",
    "systeminformation": "^5.22.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@playwright/test": "^1.58.1",
    "@types/node": "^20.11.24",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json}": "biome check --write"
  }
}
